{
    "manifest_version" : 3,
    "name" : "test",
    "version" : "1.0.0",
    "description" : "test",
    "icons": {
        "32": "icon32.png"
    },
    "background": {
        "service_worker" : "service-worker.js",
        "type" : "module"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["content-script.js"]
        }
    ],
    "action" : { 
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "scripting","tabs","activeTab"
    ],
    "host_permissions" : [
        "https://*/*",
        "http://*/*"
    ]
}